nfunction router() {\r\n    const routePath = location.hash;\r\n\r\n    if (routePath === '') {\r\n        newsFeed();\r\n    } else {\r\n        newsDetail();\r\n    }\r\n}\r\n\r\nwindow.addEventListener('hashchange', router);\r\n\r\nrouter();\r\n"},"lineCount":null}},"error":null,"hash":"73ce8614fd2a4a52107f0dba9626eea6","cacheData":{"env":{}}}